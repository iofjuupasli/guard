{
  "name": "guard-bit",
  "version": "1.0.3",
  "description": "Utility to split opportunities by the account level (free\\pro\\enterprise\\etc.) for frontend and node",
  "main": "guard.js",
  "keywords": [
    "access",
    "control",
    "acl",
    "guard",
    "auth",
    "authorized",
    "unauthorized"
